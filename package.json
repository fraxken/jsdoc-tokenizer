{
    "name": "jsdoc-tokenizer",
    "version": "1.4.1",
    "description": "Tokenize (Scan) JSDoc buffer",
    "main": "index.js",
    "scripts": {
        "prepublishOnly": "pkg-ok",
        "test": "ava --verbose",
        "coverage": "nyc npm test",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fraxken/jsdoc_tokenizer.git"
    },
    "keywords": [
        "jsdoc",
        "token",
        "tokenizer",
        "lexer",
        "parser",
        "scanner",
        "scan"
    ],
    "files": [
        "src",
        "index.js",
        "index.d.ts"
    ],
    "author": "fraxken",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/fraxken/jsdoc_tokenizer/issues"
    },
    "homepage": "https://github.com/fraxken/jsdoc_tokenizer#readme",
    "dependencies": {},
    "devDependencies": {
        "@commitlint/cli": "^8.1.0",
        "@commitlint/config-conventional": "^8.1.0",
        "@slimio/eslint-config": "^3.0.3",
        "@slimio/is": "^1.5.0",
        "ava": "^2.3.0",
        "eslint": "^6.3.0",
        "husky": "^3.0.5",
        "nyc": "^14.1.1",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=10"
    },
    "husky": {
        "hooks": {
            "pre-push": "npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}
